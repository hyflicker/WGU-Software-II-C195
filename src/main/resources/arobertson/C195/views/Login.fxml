<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17"
      fx:controller="arobertson.C195.Controllers.LoginController">
    <AnchorPane prefHeight="402.0" prefWidth="644.0" VBox.vgrow="ALWAYS">
        <Label fx:id="title" layoutX="298.0" layoutY="59.0" text="Login">
            <font>
                <Font name="System Bold" size="34.0"/>
            </font>
        </Label>
        <AnchorPane layoutX="6.0" layoutY="149.0" prefHeight="83.0" prefWidth="536.0">
            <Label fx:id="username_label" alignment="CENTER_RIGHT" prefHeight="38.0" prefWidth="269.0"
                   text="Username" AnchorPane.bottomAnchor="45.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="267.0" AnchorPane.topAnchor="0.0">
                <font>
                    <Font size="26.0"/>
                </font>
            </Label>
            <TextField fx:id="username" layoutX="283.0" layoutY="7.0" prefHeight="25.0" prefWidth="214.0"
                       promptText="Enter Username"/>
            <Label fx:id="password_label" alignment="CENTER_RIGHT" layoutY="45.0" prefHeight="38.0"
                   prefWidth="269.0" text="Password">
                <font>
                    <Font size="26.0"/>
                </font>
            </Label>
            <PasswordField fx:id="password" layoutX="283.0" layoutY="52.0" prefHeight="25.0"
                           prefWidth="214.0" promptText="Enter Password"/>
        </AnchorPane>
        <ButtonBar layoutX="271.0" layoutY="247.0" prefHeight="25.0" prefWidth="234.0">
            <buttons>
                <Button fx:id="login_btn" mnemonicParsing="false" onAction="#setLogin_btn" prefHeight="25.0"
                        prefWidth="130.0" text="Login" textAlignment="CENTER"/>
                <Button fx:id="exit_btn" mnemonicParsing="false" prefHeight="25.0" prefWidth="104.0" text="Exit"
                        textAlignment="CENTER"/>
            </buttons>
        </ButtonBar>
        <Label fx:id="timezone_label" alignment="CENTER_RIGHT" layoutX="49.0" layoutY="295.0" prefHeight="38.0"
               prefWidth="234.0">
            <font>
                <Font size="26.0"/>
            </font>
        </Label>
        <Label fx:id="location_label" layoutX="287.0" layoutY="300.0">
            <font>
                <Font size="19.0"/>
            </font>
        </Label>
    </AnchorPane>
</VBox>

